<h1>10 Closest Electric and Propane Stations within 6 miles!</h1>
<% @stations.each do |station| %>
<div class="Station">
  <ul>
    <li>Name: <%= station.name %></li>
    <li>Address: <%= station.address %></li>
    <li>Fuel Types: <%= station.fuel_types %></li>
    <li>Distance: <%= station.distance %> miles away</li>
    <li>Access Times: <%= station.access_times %></li>
  </ul>
</div>
<% end %>
